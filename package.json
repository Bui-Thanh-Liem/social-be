{
  "name": "nodejs-social",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start:dev": "concurrently --names \"SERVER,WORKER\" \"npm run start:server\" \"npm run start:worker\"",
    "start:server": "cross-env NODE_ENV=development nodemon",
    "start:worker": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register src/bull/workers/worker.ts",
    "build": "rimraf ./dist && tsc && tsc-alias && copyfiles -u 1 src/**/*.handlebars dist",
    "start:prod": "concurrently \"npm run start:server:prod\" \"npm run start:worker:prod\"",
    "start:server:prod": "node dist/server.js",
    "start:worker:prod": "node dist/bull/workers/worker.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "sync:shared": "rm -rf ../fe/src/shared && cp -r ./src/shared ../fe/src/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/compression": "^1.8.1",
